<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%p from %c at %d{dd/MM/yyyy HH:mm:ss} %m%n"/>
        </Console>
  	</Appenders>
        <Loggers>
            <Root level="ERROR">
                <Appender-Ref ref="console"/>
            </Root>
        </Loggers>
    </Appenders>
    <Loggers>
    	<Root level="WARN">
            <Appender-Ref ref="console"/>
        </Root>
    </Loggers>
